---
const { title, url, startDate, endDate } = Astro.props
---
<div class="card">
    <a class="card-link" href={url}>
        <div class="p-2">
            <div class="text-2xl font-semibold">{title}</div>
            <div class="flex space-x-2"><p class="text-accent">Started:</p><p>{startDate.toLocaleDateString()}</p></div>
            <div class="flex space-x-2"><p class="text-accent">Returned:</p><p>{endDate.toLocaleDateString()}</p></div>
        </div>
    </a>
</div>